{
    "env": {
        "es2020": true
    },
    "extends": [
        "airbnb-base"
        // "plugin:@typescript-eslint/recommended",
    //    "prettier/@typescript-eslint",
    //    "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
        // "prettier"
    ],
    "rules": {
        "no-unused-vars": "off",
        "camelcase":"off",
        "@typescript-eslint/camelcase": "off",
        "class-methods-use-this": "off",
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "ts": "never"
            }
        ]
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    }
}
